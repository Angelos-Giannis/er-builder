# Definition of tables.
[Country]
	*id
	name {label: "string"}

[League]
	*id
	+country_id {label: "int"}
	name {label: "string"}
	code {label: "string"}

[Team]
	*id
	name {label: "string"}
	+league_id {label: "int"}


# Definition of foreign keys.
league *--* country {label: "country_id"}
team *--* league {label: "league_id"}
